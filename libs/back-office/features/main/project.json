{
  "root": "libs/back-office/features/main",
  "sourceRoot": "libs/back-office/features/main/src",
  "projectType": "library",
  "tags": ["scope:back-office", "type:features"],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/back-office/features/main/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/back-office/features/main"],
      "options": {
        "jestConfig": "libs/back-office/features/main/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
